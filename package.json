{
  "name": "remark-highlight-mark",
  "version": "0.0.1",
  "description": "remark extension to support highlight mark",
  "license": "MIT",
  "keywords": [
    "micromark",
    "micromark-extension",
    "remark",
    "remark-extension",
    "mark",
    "highlight",
    "markdown",
    "unified"
  ],
  "sideEffects": false,
  "type": "module",
  "files": [
    "lib/",
    "src/"
  ],
  "exports": {
    "development": "./src/index.ts",
    "default": "./src/index.ts",
    "types": "./src/index.ts"
  },
  "dependencies": {
    "mdast-util-from-markdown": "^2.0.1",
    "mdast-util-to-markdown": "^2.1.0",
    "micromark-util-chunked": "^2.0.0",
    "micromark-util-classify-character": "^2.0.0",
    "micromark-util-resolve-all": "^2.0.0",
    "micromark-util-symbol": "^2.0.0",
    "micromark-util-types": "^2.0.0",
    "uvu": "^0.5.6"
  },
  "devDependencies": {
    "@nshq/tsconfig": "^1.0.0",
    "@tsconfig/node-lts": "^20.1.3",
    "@types/mdast": "^4.0.4",
    "@types/node": "^20.14.13",
    "c8": "^7.14.0",
    "micromark": "^4.0.0",
    "micromark-build": "^2.0.2",
    "remark-cli": "^12.0.1",
    "remark-preset-wooorm": "^10.0.0",
    "tape": "^5.8.1",
    "type-coverage": "^2.29.1",
    "typescript": "^5.5.4",
    "unified": "^11.0.5",
    "unist-util-remove-position": "^5.0.0"
  },
  "scripts": {
    "prepack": "pnpm build && pnpm format",
    "build": "tsc --build --clean && tsc --build && type-coverage && micromark-build",
    "test-api-dev:micromark": "node --conditions development test/micromark.js",
    "test-api-dev:mdast-util": "node --conditions development test/mdast-util.js",
    "test-api-dev": "node --conditions development test/index.js",
    "test-api": "pnpm test-api-dev",
    "test-coverage": "c8 --100 --reporter lcov pnpm test-api",
    "test": "pnpm build && pnpm format && pnpm test-coverage"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-wooorm"
    ]
  },
  "typeCoverage": {
    "atLeast": 100,
    "detail": true,
    "strict": true,
    "ignoreCatch": true
  }
}
